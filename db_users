CREATE USER IF NOT EXISTS 'APIDeUsuarios'@'%' IDENTIFIED BY '12345678';
GRANT SELECT, INSERT, UPDATE ON Digitality.* TO 'APIDeUsuarios'@'%';

CREATE USER IF NOT EXISTS 'Backoffice'@'%' IDENTIFIED BY '12345678';
GRANT ALL PRIVILEGES ON Digitality.* TO 'Backoffice'@'%';

CREATE USER IF NOT EXISTS 'APIDeCooperativa'@'%' IDENTIFIED BY '12345678';
GRANT SELECT, INSERT, UPDATE ON Digitality.* TO 'APIDeCooperativa'@'%';

FLUSH PRIVILEGES;
